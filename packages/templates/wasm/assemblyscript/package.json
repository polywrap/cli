{
  "name": "template-wasm-as",
  "description": "Polywrap AssemblyScript Wrapper Template",
  "private": true,
  "version": "0.12.1",
  "scripts": {
    "codegen": "npx polywrap codegen",
    "build": "npx polywrap build",
