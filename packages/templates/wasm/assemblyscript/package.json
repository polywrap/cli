  },
  "devDependencies": {
    "@types/jest": "26.0.8",
    "jest": "26.6.3",
    "polywrap": "0.12.1",
    "ts-jest": "26.5.4",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@polywrap/wasm-as": "0.12.1",
    "assemblyscript": "0.19.23"
  }
}
